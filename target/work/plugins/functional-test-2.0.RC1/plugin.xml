<plugin name='functional-test' version='2.0.RC1' grailsVersion='1.3 &gt; *'>
  <author>Marc Palmer</author>
  <authorEmail>marc@grailsrocks.com</authorEmail>
  <title>Functional Testing</title>
  <description>Simple 'pure grails' functional testing for your web applications
</description>
  <documentation>http://grails.org/plugin/functional-test</documentation>
  <type>FunctionalTestGrailsPlugin</type>
  <resources>
    <resource>FunctionalTestUrlMappings</resource>
    <resource>com.grailsrocks.functionaltest.controllers.FunctionalTestDataAccessController</resource>
    <resource>com.grailsrocks.functionaltest.controllers.test.SelfTestController</resource>
  </resources>
  <repositories>
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='http://repository.codehaus.org' url='http://repository.codehaus.org/' />
    <repository name='http://repository.jboss.org/maven2' url='http://repository.jboss.org/maven2/' />
    <repository name='grailsCentral' url='http://plugins.grails.org' />
    <repository name='http://repo.grails.org/grails/plugins' url='http://repo.grails.org/grails/plugins/' />
    <repository name='http://repo.grails.org/grails/core' url='http://repo.grails.org/grails/core/' />
    <repository name='grailsCore' url='http://svn.codehaus.org/grails/trunk/grails-plugins' />
  </repositories>
  <dependencies>
    <test>
      <dependency group='xalan' name='xalan' version='2.7.1' />
      <dependency group='xalan' name='serializer' version='2.7.1' />
      <dependency group='org.w3c.css' name='sac' version='1.3' />
      <dependency group='net.sourceforge.cssparser' name='cssparser' version='0.9.7' />
      <dependency group='commons-codec' name='commons-codec' version='1.6' />
      <dependency group='net.sourceforge.nekohtml' name='nekohtml' version='1.9.16' />
    </test>
    <compile>
      <dependency group='org.apache.httpcomponents' name='httpclient' version='4.2.1' />
      <dependency group='net.sourceforge.htmlunit' name='htmlunit-core-js' version='2.10' />
      <dependency group='org.codehaus.groovy.modules.http-builder' name='http-builder' version='0.5.2' />
      <dependency group='net.sourceforge.htmlunit' name='htmlunit' version='2.10' />
    </compile>
  </dependencies>
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>